{
    "configurations": [
        {
            "name": "lishui",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": ["USE_HAL_DRIVER", "STM32F103x6", "ARM_MATH_CM3"],
            "compilerPath": "${env:ARMGCC_DIR}/arm-none-eabi-gcc",
            "compilerArgs": ["-mcpu=cortex-m3", "-mthumb"],
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}
